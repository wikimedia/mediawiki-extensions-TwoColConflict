	"EventLoggingSchemas": {
		"TwoColConflictConflict": 18155295
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "TwoColConflict"
	},
	"SpecialPages": {
		"SimulateTwoColEditConflict": "TwoColConflict\\SpecialConflictTestPage\\SpecialConflictTestPage"
	}
